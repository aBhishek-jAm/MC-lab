#include<stdio.h>
#include<stdlib.h>
void main(){
    char str[20];
    printf("enter the string\n");
    scanf("%s",str);
    printf("\noriginal string %s",str);
    for(int i=0;str[i]!='\0';i++){
        if(str[i]>=97 && str[i]<=122)
        str[i]-=32;
        else
        str[i]+=32;
    }
    printf("\nconverted string %s",str);

}
